{
  "name": "my-quick-credit",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel src -d build",
    "clean": "rm -rf build .nyc_output coverage && mkdir build",
    "dev": "nodemon --exec babel-node src/server.js",
    "start": "node --exec babel-node src/server.js",
    "test": "mocha --require @babel/register --recursive --exit",
    "test:coverage": "cross-env NODE_ENV=test nyc --reporter=text npm test",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm run test:coverage && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "assert": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-airbnb": "^3.2.1",
    "babel-watch": "^7.0.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "import": "0.0.6",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "express-validator": "^5.3.1",
    "moment": "^2.24.0",
    "password-hash": "^1.2.2",
    "request": "^2.88.0",
    "uuid": "^3.3.2"
  }
}
